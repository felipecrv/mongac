INT ID(a) PTVIRG
CHAR ID(b) PTVIRG

VOID ID(func) APAR INT ID(a) FPAR ACHAVE
    INT ID(b) PTVIRG
    IF APAR ID(a) FPAR ACHAVE
        RETURN PTVIRG
    FCHAVE ELSE ID(b) ATRIB ID(a) PTVIRG
    WHILE APAR ID(a) IGUAL ID(b) OU NAO APAR ID(a) MAIORQ ID(b) FPAR E NUMINT(1) MENORQ NUMINT(2) FPAR ACHAVE
        RETURN NEW CHAR ACOL NUMINT(5) FCOL PTVIRG
    FCHAVE
 
    FLOAT ID(f) PTVIRG
    ID(f) ATRIB NUMFLOAT(1.) PTVIRG
    ID(f) ATRIB NUMFLOAT(10.67) VIRG ID(b) ATRIB NUMINT(0xdeadBEEF) PTVIRG
    ID(a) ATRIB ID(b) VIRG ID(b) ATRIB ID(a) PTVIRG
    ID(a) ATRIB ID(a) SOMA ID(b) MULT ID(a) DIV ID(b) SUB NUMINT(1) PTVIRG
    INT ID(z) ATRIB ID(a) MENORIG ID(b) OU ID(a) MAIORIG ID(b) PTVIRG
    CHAR ACOL FCOL ID(s) PTVIRG
    ID(s) ATRIB STRING("Felipe") PTVIRG
    ID(s) ATRIB STRING("foo\"bar") PTVIRG
    ID(s) ATRIB STRING("multiline\
string\"\t\n") PTVIRG
